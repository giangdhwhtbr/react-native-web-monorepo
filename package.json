{
  "name": "myprj",
  "version": "0.0.1",
  "private": true,
  "workspaces": {
    "packages": [
      "packages/*",
      "subdomains/*"
    ]
  },
  "scripts": {
    "android": "yarn workspace @myprj/mobile android",
    "compile": "tsc -b --incremental",
    "ios": "yarn workspace @myprj/mobile ios",
    "studio": "yarn workspace @myprj/mobile studio",
    "xcode": "yarn workspace @myprj/mobile xcode",
    "web": "yarn workspace @myprj/web start"
  },
  "dependencies": {
    "react-native": "*"
  },
  "devDependencies": {
    "typescript": "3.9.5"
  }
}
